<div id="authentication-form">

<h2 id="authentication-title">Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

  <div id="devise-errors"
    <%= devise_error_messages! %>
  </div>

  <div class="authentication-field">
    <!-- <%= f.label :first_name,'First Name' %><br /> -->
    <%= f.text_field :first_name, placeholder: "First Name" %>
  </div>

  <div class="authentication-field">
    <!-- <%= f.label :last_name, 'Last Name' %><br /> -->
    <%= f.text_field :last_name, placeholder: "Last Name" %>
  </div>

  <div class="authentication-field">
    <!-- <%= f.label :email %><br /> -->
    <%= f.email_field :email, placeholder: "Email", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="authentication-field">
    <!-- <%= f.label :password %> -->
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, placeholder: "Password", autocomplete: "off" %>
  </div>

  <div class="authentication-field">
    <!-- <%= f.label :password_confirmation %><br /> -->
    <%= f.password_field :password_confirmation, placeholder: "Password Confirmation", autocomplete: "off" %>
  </div>

  <div class="authentication-actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>
</div>

<div id="shared-links">
  <%= render "devise/shared/links" %>
</div>
